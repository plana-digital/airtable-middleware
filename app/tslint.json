{
	"defaultSeverity": "error",
	"extends": [
		"tslint:latest",
		"tslint-config-prettier"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**"
		]
	},
	"rules": {
		"quotemark": [
			true,
			"double"
		],
		"indent": [
			true,
			"tabs",
			4
		],
		"interface-name": false,
		"ordered-imports": false,
		"no-submodule-imports": false,
		"no-console": false,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false,
		"trailing-comma": false
	}
}
